    $('.tabbed-content').each(function () {
        if (!$('li', this).eq(0).hasClass('active') || !$('li', this).eq(2).hasClass('active') || !$('li', this).eq(3).hasClass('active') || !$('li', this).eq(4).hasClass('active')) {
            $('li', this).eq(0).addClass('active');
