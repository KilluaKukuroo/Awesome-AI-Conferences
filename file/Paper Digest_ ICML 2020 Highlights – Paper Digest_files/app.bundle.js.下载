            _duration = _settings.duration ? _settings.duration : 150,
            _options = {
            type: _type,
            duration: _duration,
            file: _settings.svg
        };

        var _vivus = new Vivus($(this)[0], _options);

        if (_settings.hover) {
            $(this).parent().parent().on('mouseenter', function () {
                _vivus.stop().reset().play(2);
